# Simple test script for the elexis-vagrant project to restore
# the elexis database into the main database.
dump = ARGV[0] if ARGV.size > 0

cmd  = "gunzip --to-stdout #{dump} | "
cmd += "psql --user elexis --host localhost --dbname=#{pg_main_db_name}"

system(cmd)
logAction("load/restore of #{pg_main_db_name} from file #{dump} finished")
